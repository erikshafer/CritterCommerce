    <ItemGroup>
        <PackageReference Include="Marten" Version="8.3.1" />
        <PackageReference Include="WolverineFx" Version="4.5.3" />
        <PackageReference Include="WolverineFx.Http.FluentValidation" Version="4.5.3" />
